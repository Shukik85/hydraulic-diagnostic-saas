{
  "name": "hydraulic-diagnostic-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .ts,.js,.vue --fix",
    "lint:check": "eslint . --ext .ts,.js,.vue",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "vue-tsc --noEmit",
    "analyze": "nuxi analyze",
    "clean": "rm -rf .nuxt .output dist node_modules/.cache"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.2",
    "nuxt": "^4.1.3",
    "pinia": "^2.3.0",
    "vue": "^3.5.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/icon": "^1.8.0",
    "@nuxt/fonts": "^0.12.0",
    "@nuxtjs/seo": "^2.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@types/node": "^24.9.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.1"
  }
}