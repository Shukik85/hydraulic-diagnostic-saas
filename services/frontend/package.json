{
  "name": "hydraulic-diagnostics-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate:api": "openapi-typescript-codegen --input ../../specs/combined-api.json --output ./generated/api --client axios --useOptions --useUnionTypes",
    "validate:api": "swagger-cli validate ../../specs/combined-api.json",
    "predev": "npm run generate:api",
    "dev": "nuxt dev",
    "prebuild": "npm run generate:api",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@nuxt/image": "^1.7.0",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "axios": "^1.6.5",
    "nuxt": "^3.12.4",
    "pinia": "^2.1.7",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/test-utils": "^3.13.1",
    "@playwright/test": "^1.45.0",
    "@types/node": "^20.14.10",
    "eslint": "^9.6.0",
    "openapi-merge-cli": "^1.3.1",
    "openapi-typescript-codegen": "^0.29.0",
    "swagger-cli": "^4.0.4",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.26"
  }
}
