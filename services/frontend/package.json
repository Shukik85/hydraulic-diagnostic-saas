    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^13.17.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^15.11.7",
