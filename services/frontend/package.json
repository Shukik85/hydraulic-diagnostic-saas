    "nuxt": "^4.2.1",
    "nuxt-security": "^2.5.0",
    "ofetch": "^1.4.1",
    "pinia": "^3.0.4",
    "radix-vue": "^1.9.17",
    "tailwind-merge": "^3.4.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-echarts": "^8.0.1",
    "zod": "^3.24.1"
