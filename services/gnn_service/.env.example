# Example .env file for GNN service
# Copy to .env and update with your values

# Device configuration
DEVICE=cuda

# Model parameters
HIDDEN_DIM=96
NUM_GAT_LAYERS=3
NUM_HEADS=4
LSTM_LAYERS=2
DROPOUT=0.12

# Temporal parameters
TIME_WINDOW_MINUTES=60
TIMESTEP_MINUTES=5

# Health thresholds
HEALTH_CRITICAL=0.3
HEALTH_WARNING=0.6

# Database (TimescaleDB)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=hydraulic_db
DB_USER=user
DB_PASSWORD=your_password_here

# API configuration
API_HOST=0.0.0.0
API_PORT=8001

# LLM/RAG configuration
LLM_PROVIDER=openai
LLM_API_KEY=your_openai_api_key_here
LLM_MODEL=gpt-4o-mini
LLM_TEMPERATURE=0.3

# Redis (optional, for caching)
REDIS_HOST=localhost
REDIS_PORT=6379
