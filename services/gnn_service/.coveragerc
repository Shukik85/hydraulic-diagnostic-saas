# Coverage.py configuration

[run]
source = src
omit =
    */tests/*
    */test_*.py
    */__init__.py
    */conftest.py
    */.venv/*
    */venv/*

[report]
precision = 2
show_missing = true
skip_covered = false
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
